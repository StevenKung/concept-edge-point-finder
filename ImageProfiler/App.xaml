		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Views/Style.xaml">
					<vm:ViewModelLocator x:Key="Locator" xmlns:vm="clr-namespace:ImageProfiler.ViewModels" />
				</ResourceDictionary>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
